    "clean-css": "5.3.3",
    "cssnano": "6.0.2",
    "csso": "5.0.5",
    "ejs": "^3.1.9",
    "esbuild": "0.19.11",
    "gzip-size": "^7.0.0",
    "lightningcss": "1.22.1",
    "magic.css": "^1.4.8",
    "normalize.css": "^8.0.1",
    "postcss": "8.4.32",
    "purecss": "^3.0.0",
